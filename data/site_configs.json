{
    "zonaprop.com.ar": {
        "name": "Zonaprop",
        "patterns": {
            "precio": {
                "type": "regex",
                "pattern": "(?:USD|ARS|\\$)\\s*([\\d.,]+)",
                "search_in": "text"
            },
            "moneda": {
                "type": "regex",
                "pattern": "(USD|ARS|\\$)",
                "search_in": "text",
                "transform": {
                    "$": "ARS",
                    "USD": "USD",
                    "ARS": "ARS"
                }
            },
            "metros_cuadrados_totales": {
                "type": "regex",
                "pattern": "(\\d+)\\s*m²\\s*tot\\.",
                "search_in": "html"
            },
            "metros_cuadrados_cubiertos": {
                "type": "regex",
                "pattern": "(\\d+)\\s*m²\\s*cub\\.",
                "search_in": "html"
            },
            "cantidad_dormitorios": {
                "type": "regex",
                "pattern": "(\\d+)\\s*dorm",
                "search_in": "text"
            },
            "cantidad_banos": {
                "type": "regex",
                "pattern": "(\\d+)\\s*baño",
                "search_in": "text"
            },
            "tipo_operacion": {
                "type": "regex",
                "pattern": "(venta|alquiler|alquila)",
                "search_in": "text",
                "transform": {
                    "venta": "venta",
                    "alquiler": "alquiler",
                    "alquila": "alquiler"
                }
            },
            "barrio": {
                "type": "llm_required",
                "note": "Requires LLM to extract neighborhood accurately"
            },
            "direccion": {
                "type": "llm_required",
                "note": "Requires LLM to extract address"
            }
        }
    },
    "argenprop.com": {
        "name": "Argenprop",
        "patterns": {
            "precio": {
                "type": "css_selector",
                "selector": ".titlebar__price",
                "extract": "text",
                "regex": "([\\d.,]+)"
            },
            "moneda": {
                "type": "css_selector",
                "selector": ".titlebar__price",
                "extract": "text",
                "regex": "(USD|ARS|\\$)",
                "transform": {
                    "$": "ARS",
                    "USD": "USD",
                    "ARS": "ARS"
                }
            },
            "metros_cuadrados_cubiertos": {
                "type": "regex",
                "pattern": "(\\d+)\\s*m²\\s*Cubierta",
                "search_in": "html"
            },
            "cantidad_dormitorios": {
                "type": "regex",
                "pattern": "(\\d+)\\s*dormitorio",
                "search_in": "text"
            },
            "cantidad_banos": {
                "type": "regex",
                "pattern": "(\\d+)\\s*baño",
                "search_in": "text"
            },
            "tipo_operacion": {
                "type": "regex",
                "pattern": "(Venta|Alquiler)",
                "search_in": "text",
                "transform": {
                    "Venta": "venta",
                    "Alquiler": "alquiler"
                }
            },
            "barrio": {
                "type": "llm_required",
                "note": "Requires LLM"
            },
            "direccion": {
                "type": "llm_required",
                "note": "Requires LLM"
            }
        }
    },
    "ficha.info": {
        "name": "Ficha.info",
        "patterns": {
            "precio": {
                "type": "regex",
                "pattern": "\\\\\"Sale\\\\\":\\[\\\\\"(?:USD|ARS|\\$)\\s*([\\d.,]+)\\\\\"\\]",
                "search_in": "html"
            },
            "moneda": {
                "type": "regex",
                "pattern": "\\\\\"Sale\\\\\":\\[\\\\\"(USD|ARS|\\$)",
                "search_in": "html",
                "transform": {
                    "$": "ARS",
                    "USD": "USD",
                    "ARS": "ARS"
                }
            },
            "metros_cuadrados_totales": {
                "type": "regex",
                "pattern": "\\\\\"attr\\\\\":\\\\\"total_surface\\\\\",\\\\\"value\\\\\":\\\\\"(\\d+)",
                "search_in": "html"
            },
            "metros_cuadrados_cubiertos": {
                "type": "regex",
                "pattern": "\\\\\"attr\\\\\":\\\\\"roofed_surface\\\\\",\\\\\"value\\\\\":\\\\\"(\\d+)",
                "search_in": "html"
            },
            "cantidad_dormitorios": {
                "type": "regex",
                "pattern": "\\\\\"attr\\\\\":\\\\\"suite_amount\\\\\",\\\\\"value\\\\\":\\\\\"(\\d+)",
                "search_in": "html"
            },
            "cantidad_banos": {
                "type": "regex",
                "pattern": "\\\\\"attr\\\\\":\\\\\"bathroom_amount\\\\\",\\\\\"value\\\\\":\\\\\"(\\d+)",
                "search_in": "html"
            },
            "tipo_operacion": {
                "type": "regex",
                "pattern": "\\\\\"operations\\\\\":\\{\\\\\"(Sale|Rent|TemporaryRent)\\\\\":",
                "search_in": "html",
                "transform": {
                    "Sale": "venta",
                    "Rent": "alquiler",
                    "TemporaryRent": "alquiler_temporal"
                }
            },
            "tipo_inmueble": {
                "type": "regex",
                "pattern": "\\\\\"type\\\\\":\\{\\\\\"id\\\\\":\\d+,\\\\\"name\\\\\":\\\\\"([^\\\"]+)\\\\\"",
                "search_in": "html",
                "transform": {
                    "Departamento": "departamento",
                    "Casa": "casa",
                    "PH": "casa",
                    "Local": "local",
                    "Oficina": "oficina"
                }
            },
            "direccion": {
                "type": "regex",
                "pattern": "\\\\\"address\\\\\":\\\\\"([^\\\"]+)\\\\\"",
                "search_in": "html"
            },
            "barrio": {
                "type": "llm_required",
                "note": "Extract from description or location field"
            },
            "tiene_patio": {
                "type": "regex",
                "pattern": "\\\\\"(Patio)\\\\\"",
                "search_in": "html",
                "default": false
            },
            "tiene_terraza": {
                "type": "regex",
                "pattern": "\\\\\"(Terraza|Balcón terraza|Aterrazado)\\\\\"",
                "search_in": "html",
                "default": false
            },
            "tiene_balcon": {
                "type": "regex",
                "pattern": "\\\\\"(Balcón)\\\\\"",
                "search_in": "html",
                "default": false
            },
            "tiene_cochera": {
                "type": "regex",
                "pattern": "\\\\\"(Cochera|parking_lot_amount)\\\\\"",
                "search_in": "html",
                "default": false
            },
            "tiene_pileta": {
                "type": "regex",
                "pattern": "\\\\\"(Pileta|Piscina)\\\\\"",
                "search_in": "html",
                "default": false
            },
            "tiene_quincho": {
                "type": "regex",
                "pattern": "\\\\\"(Quincho|Parrilla)\\\\\"",
                "search_in": "html",
                "default": false
            }
        }
    },
    "alagnapropiedades.com.ar": {
        "name": "Alagna Propiedades",
        "patterns": {
            "precio": {
                "type": "regex",
                "pattern": "Precio de Venta: U\\$S\\s*([\\d.,]+)",
                "search_in": "text"
            },
            "moneda": {
                "type": "regex",
                "pattern": "(U\\$S)",
                "search_in": "text",
                "transform": {
                    "U$S": "USD"
                }
            },
            "metros_cuadrados_totales": {
                "type": "regex",
                "pattern": "Superficie Total\\s*(\\d+)(?:[.,]\\d+)?\\s*m\\s*2",
                "search_in": "text"
            },
            "metros_cuadrados_cubiertos": {
                "type": "regex",
                "pattern": "Superficie cubierta:\\s*(\\d+)(?:[.,]\\d+)?\\s*m\\s*2",
                "search_in": "text"
            },
            "cantidad_dormitorios": {
                "type": "regex",
                "pattern": "Dormitorios\\s*(\\d+)",
                "search_in": "text"
            },
            "cantidad_banos": {
                "type": "regex",
                "pattern": "Baños\\s*(\\d+)",
                "search_in": "text"
            },
            "tiene_cochera": {
                "type": "regex",
                "pattern": "Cochera\\s*(\\d+)",
                "search_in": "text"
            },
            "tipo_operacion": {
                "type": "regex",
                "pattern": "(venta|alquiler)",
                "search_in": "url",
                "transform": {
                    "venta": "venta",
                    "alquiler": "alquiler"
                }
            },
            "barrio": {
                "type": "llm_required",
                "note": "Extract from description"
            },
            "direccion": {
                "type": "llm_required",
                "note": "Extract from description"
            }
        }
    },
    "_comments": {
        "note": "Configuration file for site-specific data extraction",
        "pattern_types": [
            "regex: Use regular expression to extract from text or HTML",
            "css_selector: Use CSS selector to find element, then apply regex if needed",
            "llm_required: Mark field as requiring LLM processing"
        ],
        "search_in_options": [
            "text: Search in clean text content",
            "html: Search in raw HTML"
        ]
    }
}